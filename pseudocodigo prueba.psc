Algoritmo sin_titulo
	definir n1, n2, n3, aux Como Entero
	
	escribir "Ingresar el valor en n1"
	leer n1
	escribir "Ingresar el valor en n2"
	leer n2
	escribir "Ingresar el valor en n3"
	leer n3
	escribir "valores ingresados"
	escribir "n1: ", n1, ", n2: ", n2, ", n3: ", n3
	
	aux = n1
	n1 = n2
	n2 = n3
	n3 = aux
	
	escribir "Valores luego del desplazamiento contrario"
	escribir "n1: ", n1, ", n2: ", n2, ", n3: ", n3
	
	escribir "Desplazamiento completo"
	
FinAlgoritmo
